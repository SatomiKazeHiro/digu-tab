{
    "manifest_version": 2,
    "name": "DiguTab",
    "version": "1.0",
    "description": "Digu自定义系列的新标签页扩展",
    "chrome_url_overrides": {
        "newtab": "digutab.html"
    },
    "permissions": [
        "tabs",
        "storage"
     ],
    "icons": {
        "64": "./icons/64.png"
    },
    "offline_enabled": false
}