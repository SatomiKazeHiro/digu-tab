{
  "manifest_version": 2,
  "name": "Weknow Tab",
  "version": "1.0",
  "description": "为知新标签页",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
